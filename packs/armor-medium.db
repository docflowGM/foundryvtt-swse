{"_id":"armor-imperial-knight-armor","name":"Imperial Knight Armor","type":"armor","img":"icons/svg/shield.svg","system":{"armorType":"medium","defenseBonus":8,"maxDexBonus":3,"armorCheckPenalty":-5,"fortBonus":2,"speedPenalty":2,"weight":16,"cost":7000,"equipped":false,"description":"<p>Distinctive armor worn by the Imperial Knights who serve the Fel Empire. Combines protection with Force-user mobility.</p>","sourcebook":"Legacy Era Campaign Guide","availability":"Rare","costNumeric":7000,"reflexBonus":8,"fortitudeBonus":2,"maxDex":3,"equipmentPerceptionBonus":0,"armorProficiency":false,"features":""},"effects":[],"folder":null,"sort":0,"ownership":{"default":0},"flags":{}}
