conscious.", "costNumeric": null, "tree": "Sense", "treeId": "d6b4331c0e31f7e3"}, "effects": [{"name": "Force Perception (Trained)", "icon": "icons/svg/item-bag.svg", "changes": [{"key": "system.skills.perception.trained", "mode": 5, "value": "true"}], "disabled": false, "duration": {}, "flags": {"swse": {"source": "talent", "talentName": "Force Perception", "effectType": "skill-trained"}}}], "folder": null, "sort": 0, "ownership": {"default": 0}, "flags": {"swse": {"id": "swse.talent.force_perception"}}}
