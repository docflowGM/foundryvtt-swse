<div class="swse-dialog-body">
  <div class="swse-dialog-content">
    {{{content}}}
  </div>

  <div class="swse-dialog-buttons dialog-buttons">
    {{#each buttons}}
      <button type="button" class="btn {{cssClass}}" data-action="{{action}}">
        {{{icon}}} {{label}}
      </button>
    {{/each}}
  </div>
</div>
