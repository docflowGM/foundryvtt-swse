{
  "name": "swse",
  "title": "Star Wars Saga Edition (SWSE)",
  "description": "A Foundry VTT system for Star Wars Saga Edition, featuring dynamic character, droid, and vehicle sheets with template-driven defaults and modular scripting.",
  "version": "1.0.0",
  "license": "CC BY-SA 4.0",
  "minimumCoreVersion": "11",
  "compatibleCoreVersion": "13",
  "author": "DocflowGM",
  "authors": [
    {
      "name": "DocflowGM",
      "url": "https://github.com/docflowGM"
    }
  ],
  "url": "https://github.com/docflowGM/foundryvtt-swse",
  "manifest": "https://raw.githubusercontent.com/docflowGM/foundryvtt-swse/main/system.json",
  "download": "https://github.com/docflowGM/foundryvtt-swse/archive/refs/heads/main.zip",
  "esmodules": [
    "scripts/init.js",
    "scripts/races.js",
    "scripts/swse-actor.js",
    "scripts/swse-droid.js",
    "scripts/swse-vehicle.js",
    "scripts/swse.js"
  ],
  "styles": [
    "styles/swse.css"
  ],
  "packs": [
    {
      "name": "swse-armor",
      "label": "SWSE Armor",
      "path": "packs/Armor.json",
      "entity": "Item"
    },
    {
      "name": "swse-classes",
      "label": "SWSE Classes",
      "path": "packs/classes.json",
      "entity": "Item"
    },
    {
      "name": "swse-equipment",
      "label": "SWSE Equipment",
      "path": "packs/equipment.json",
      "entity": "Item"
    },
    {
      "name": "swse-feats",
      "label": "SWSE Feats",
      "path": "packs/feats.json",
      "entity": "Item"
    },
    {
      "name": "swse-forcepowers",
      "label": "SWSE Force Powers",
      "path": "packs/forcepowers.json",
      "entity": "Item"
    },
    {
      "name": "swse-talents",
      "label": "SWSE Talents",
      "path": "packs/talents.json",
      "entity": "Item"
    },
    {
      "name": "swse-weapons",
      "label": "SWSE Weapons",
      "path": "packs/weapons.json",
      "entity": "Item"
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "gridDistance": 1,
  "gridUnits": "m",
  "preloadTemplates": [
    "templates/actor/character-sheet.hbs",
    "templates/actor/droid-sheet.hbs",
    "templates/actor/vehicle-sheet.hbs"
  ]
}
