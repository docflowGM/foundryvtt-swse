{
  "name": "swse",
  "title": "Star Wars Saga Edition",
  "description": "A FoundryVTT system for Star Wars Saga Edition.",
  "version": "1.0.0",
  "author": "Doc Flow",
  "compatibility": {
    "minimum": "11",
    "verified": "13"
  },
  "scripts": [
    "scripts/init.js",
    "scripts/swse-data.js",
    "scripts/swse-actor.js",
    "scripts/swse-droid.js",
    "scripts/swse-item.js",
    "scripts/swse-vehicle.js",
    "scripts/swse-levelup.js",
    "scripts/races.js",
    "scripts/chargen.js",
    "scripts/dice-utils.js",
    "scripts/diceroller.js",
    "scripts/load-templates.js"
  ],
  "esmodules": [
    "system/swse.js"
  ],
  "styles": [
    "styles/swse.css",
    "styles/swse-levelup.css",
    "styles/swse-vehicles.css"
  ],
  "packs": [
    {
      "name": "swse-classes",
      "label": "SWSE Classes",
      "path": "packs/swse-classes.db",
      "type": "Item"
    },
    {
      "name": "swse-equipment",
      "label": "SWSE Equipment",
      "path": "packs/swse-equipment.db",
      "type": "Item"
    },
    {
      "name": "swse-feats",
      "label": "SWSE Feats",
      "path": "packs/swse-feats.db",
      "type": "Item"
    },
    {
      "name": "swse-talents",
      "label": "SWSE Talents",
      "path": "packs/swse-talents.db",
      "type": "Item"
    },
    {
      "name": "swse-vehicles",
      "label": "SWSE Vehicles",
      "path": "packs/swse-vehicles.db",
      "type": "Item"
    }
  ],
  "schemas": [
    "schema/skills.schema.json",
    "schema/conditions.schema.json",
    "schema/extraSkillUses.schema.json",
    "schema/combat-action.schema.json"
  ],
  "system": [
    "system/config.js",
    "system/actor-sheet.html",
    "system/item-sheet.html"
  ],
  "template": [
    "templates"
  ],
  "license": "LICENSE.txt",
  "url": "https://github.com/docflowGM/foundryvtt-swse",
  "manifest": "https://raw.githubusercontent.com/docflowGM/foundryvtt-swse/main/system.json",
  "download": "https://github.com/docflowGM/foundryvtt-swse/archive/refs/heads/main.zip"
}
